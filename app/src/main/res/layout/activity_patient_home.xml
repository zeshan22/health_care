<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_weight="25"

    android:background="@drawable/abs"
    tools:context=".PatientHome">

    <include layout="@layout/app_bar_layout" android:id="@+id/main_page_toolbar" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/patient_home_recycler"
        android:layout_below="@+id/main_page_toolbar"
        android:layout_alignParentLeft="true"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="15"></android.support.v7.widget.RecyclerView>

</LinearLayout>
